gunit HaskellExpressions;

expression:
"1234" -> "1234"
"-1234" -> (- 1234)
"1+1" -> (+ 1 1)
"2*2" -> (* 2 2)
"1==1" -> (== 1 1)
"1*1+2" -> (+ (* 1 1) 2)
"1+1*2" -> (+ 1 (* 1 2))
"1^2^3" -> (^ 1 (^ 2 3))
"2^3^^4**5" -> (^ 2 (^^ 3 (** 4 5)))
"1*-2+3" -> (+ (* 1 (- 2)) 3)
"1:2>>=f>>g" -> (>> (>>= (: 1 2) f) g)
"1+1==2&&True||False" -> (|| (&& (== (+ 1 1) 2) True) False)
"f.g$h" -> ($ (. f g) h)
